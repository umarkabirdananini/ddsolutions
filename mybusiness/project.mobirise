          "_JSplaceholders": [
            "<script>\n        const checkboxes = document.querySelectorAll('.mobirise-custom-services .service');\n        const totalElement = document.getElementById('total');\n        const offerCheckbox = document.getElementById('limited-offer');\n        const individualServicesCheckbox = document.getElementById('individual-services');\n        const offerNote = document.querySelector('.mobirise-custom-services .offer-note');\n        const serviceNote = document.querySelector('.mobirise-custom-services .service-note');\n\n        // Listen for changes in the service option selection\n        offerCheckbox.addEventListener('change', handleServiceOptionSelection);\n        individualServicesCheckbox.addEventListener('change', handleServiceOptionSelection);\n\n        function handleServiceOptionSelection() {\n            if (offerCheckbox.checked) {\n                // If offer is selected, disable all individual checkboxes and set total cost to 200,000\n                checkboxes.forEach(checkbox => {\n                    checkbox.checked = false;\n                    checkbox.disabled = true;\n                });\n                totalElement.textContent = '200,000';\n                serviceNote.style.display = 'none';\n                offerNote.style.display = 'block';\n            } else {\n                // Re-enable checkboxes if Individual Services is selected\n                checkboxes.forEach(checkbox => {\n                    checkbox.disabled = false;\n                });\n                updateTotal();\n                serviceNote.style.display = 'block';\n                offerNote.style.display = 'none';\n            }\n        }\n\n        // Update total cost based on individual services selected\n        checkboxes.forEach(checkbox => {\n            checkbox.addEventListener('change', updateTotal);\n        });\n\n        function updateTotal() {\n            let total = 0;\n            checkboxes.forEach(checkbox => {\n                if (checkbox.checked) {\n                    total += parseFloat(checkbox.value);\n                }\n            });\n            totalElement.textContent = total.toFixed(2);\n        }\n\n        function sendMessage() {\n            let selectedServices = [];\n            checkboxes.forEach(checkbox => {\n                if (checkbox.checked) {\n                    selectedServices.push(checkbox.nextElementSibling.textContent.trim());\n                }\n            });\n            const total = parseFloat(totalElement.textContent);\n            const message = `Selected Services:\\n\\n` +\n                            selectedServices.join('\\n') + `\\n\\nTotal: â‚¦${total.toFixed(2)}\\n` +\n                            `Please provide further details.`;\n            const encodedMessage = encodeURIComponent(message);\n            window.open(`https://wa.me/?text=${encodedMessage}`, '_blank');\n        }\n    </script>"
