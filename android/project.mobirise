          "_customHTML": "<section data-bs-version=\"5.1\" class=\"social1\" group=\"Social\" plugins=\"ViewportChecker\">\n  <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <header>Size</header>\n    <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"0\">\n    <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n    <header>Show/Hide</header>\n    <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n    <header>Items</header>\n    <select title=\"Social Count\" name=\"iconsCount\">\n      <option value=\"1\">1</option>\n      <option value=\"2\">2</option>\n      <option value=\"3\">3</option>\n      <option value=\"4\" selected>4</option>\n      <option value=\"5\">5</option>\n    </select>\n    <input type=\"color\" title=\"Icons\" name=\"iconsColor\" value=\"#e43f3f\">\n    <input type=\"color\" title=\"Icons Hover\" name=\"iconHover\" value=\"#909190\">\n    <input type=\"color\" title=\"Circle Color\" name=\"circleColor\" value=\"#ffffff\">\n    <header>Background</header>\n    <fieldset type=\"background\" name=\"bg\">\n      <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/jewelrym5/components/_images/background1.jpg\">\n      <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n    </fieldset>\n    <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n    <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay&&bg.type !== 'color'\">\n    <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay&&bg.type !== 'color'\">\n    <!-- End block parameters -->\n  </mbr-parameters>\n\n  <div class=\"mbr-overlay\" mbr-if=\"overlay&&bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\"></div>\n  <div class=\"container\">\n    <h3 class=\"mbr-section-title align-center mb-3 mbr-fonts-style\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title, .social-media\"><b>Have a Problem?&nbsp;</b><br><b>Lets Chat</b></h3>\n    <div class=\"social-media\">\n      <ul class=\"social-list\">\n        <li mbr-if=\"iconsCount > 0\">\n          <a class=\"icon-transition\" href=\"https://facebook.com/umarkabirdananini\">\n            <span mbr-icon class=\"mbr-iconfont socicon-facebook socicon\"></span>\n          </a>\n        </li>\n        <li mbr-if=\"iconsCount > 1\">\n          <a class=\"icon-transition\" href=\"https://twitter.com/umardananini\">\n            <span mbr-icon class=\"mbr-iconfont socicon-twitter socicon\" style></span>\n          </a>\n        </li>\n        <li mbr-if=\"iconsCount > 2\">\n          <a class=\"icon-transition\" href=\"https://instagram.com/umardananini\">\n            <span mbr-icon class=\"mbr-iconfont socicon-instagram socicon\"></span>\n          </a>\n        </li>\n        <li mbr-if=\"iconsCount > 3\">\n          <a class=\"icon-transition\" href=\"https://wa.me/07067874052\">\n            <span mbr-icon class=\"mbr-iconfont socicon-whatsapp socicon\"></span>\n          </a>\n        </li>\n        <li mbr-if=\"iconsCount > 4\">\n          <a class=\"icon-transition\" href=\"#\">\n            <span mbr-icon class=\"mbr-iconfont socicon-rss socicon\" style></span>\n          </a>\n        </li>\n      </ul>\n    </div>\n  </div>\n</section>",
